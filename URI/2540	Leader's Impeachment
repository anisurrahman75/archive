#include<bits/stdc++.h>
using namespace std;
int main ()
{
    int c,n,d;
float i;
    while (scanf("%d",&n) ==1)
    {
       c=0,i=0;
       i=ceil(((float)n*2)/3.0);

    for ( int j=1;j<=n;j++)
    {
       cin >>d;
        if ( d==1)
        {c++;
        }
    }
    if (c>=i)
    {
        cout<<"impeachment"<<endl;
    }
    else
    {
        cout<<"acusacao arquivada"<<endl;
    }

    }

    return 0;
}
