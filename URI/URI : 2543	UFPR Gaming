#include<iostream>
#include<stdio.h>
using namespace std;
int main ()
{
    int t,i,id,count=0,x,y;
    while(scanf("%d %d",&t,&id) ==2)
    {
        count=0;
    for ( i=1;i<=t;i++)
    {
       scanf("%d %d",&x,&y);
       if(x==id && y==0)
      count++;
    }
           printf("%d\n",count);
    }
    return 0;
}
