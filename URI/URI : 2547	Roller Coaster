#include<iostream>
#include<stdio.h>
using namespace std;
int main ()
{

    int t,max,mini,count=0,x;
    while(scanf("%d %d %d",&t,&mini,&max) ==3)
   {count=0;
       for ( int i=1;i<=t;i++)
    {scanf("%d",&x);
    if (x>=mini && x<=max)
        count++;

    }
    printf("%d\n",count);
   }

    return 0;
}
