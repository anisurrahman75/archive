#include<bits/stdc++.h>
using namespace std;
int main()
{
int arr[6][6],r,c;
for ( int i =1;i<=5;i++)
{
    for ( int j=1;j<=5;j++)
    {
        cin>>arr[i][j];
        if ( arr[i][j]==1)
        {
            r=i;
            c=j;
        }
    }


}
 int ans = abs(3-r)+abs(3-c);

    cout<<ans<<endl;


    return 0;
}
