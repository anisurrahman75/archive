#include<bits/stdc++.h>
using namespace std;
int main ()
{
     int q,n,x,ans;
     long long int sum;
     long double tem;
    cin>>q;
    while(q--)
    {
        cin>>n;
        sum=ans=0;
        tem=0.00;
        for ( int i=0; i<n; i++)
        {
            cin>>x;
            sum=sum+x;
        }

tem=(double)sum/n;
tem=ceil(tem);
ans=tem;
cout<<ans<<endl;
    }

    return 0;
}
