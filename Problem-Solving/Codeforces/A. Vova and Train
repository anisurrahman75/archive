#include<bits/stdc++.h>
using namespace std;
int main ()
{
    int t,a,b,c,d,r,s,k,le,ri;
    cin>>t;
    while(t--)
    {
        s=ri=le=k=0;
        cin>>a>>b>>c>>d;
        s= a/b;

        le=c/b;
        ri=d/b;
        if (c%b==0)
            le--;

        r=ri-le;
        k=s-r;
        cout<<k<<endl;

    }


    return 0;
}
