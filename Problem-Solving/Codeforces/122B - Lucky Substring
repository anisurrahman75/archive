#include<bits/stdc++.h>
#define ll long long

#define pb                  push_back

#define spc             prllf(" ")
#define line            prllf("\n")
#define PI              acos(-1.0)  // 3.1415926535897932
#define bitset<20>b1(a)


#define yes cout << "YES" << endl
#define no cout << "NO" << endl
#define cs(cs)      cout<<"Case "<<cs++<<": "
using namespace std;
inline void file()
{
    freopen("input.txt","r",stdin);
    freopen("output.txt","w", stdout);
}

int main()
{
    string str;
    cin>>str;
    int c=0,a=0,b=0;
    for ( int i=0;i<str.size();i++)
    {
        if (str[i]=='4') a++;
        else if (str[i]=='7')b++;
    }
    if (a==0 && b==0) cout<<-1;
    else
    {
        if (a==b) cout<<4<<endl;
        else if (a>b) cout<<4<<endl;
        else if (b>a) cout<<7<<endl;

    }

    return 0;
}
