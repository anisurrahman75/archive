#include<bits/stdc++.h>
using namespace std;
int main ()
{
    int  n, tem=0, x;
    cin>>n;
    int arr[n+1];
    for ( int i  =0 ; i <n ; i++)
    {
        cin>> x;
        arr[i]=(tem + x);
        if ( arr[i]>tem)
            tem = arr[i];


    }

    for ( int i =0;i<n;i++)
{
    cout<<arr[i]<<" ";
}

    return 0;
}
