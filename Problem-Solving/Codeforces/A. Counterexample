#include<bits/stdc++.h>
using namespace std;
int main ()
{
    long long l,r,a,b,c;
    cin>>l>>r;
    long long dif = (r-l);
    if ( dif<2)
    {
        cout<<-1<<endl;
    }
    else if ( dif==2)
    {
        if (l%2==0)
        {
            cout<<l<<" "<<l+1<<" "<<l+2<<endl;
        }
        else
        {
            cout<<-1<<endl;

        }

    }
    else
    {
        if (l%2==1)
        {
            cout<<l+1<<" "<<l+2<<" "<<l+3<<endl;

        }
        else
        {
            cout<<l<<" "<<l+1<<" "<<l+2<<endl;
        }
    }



    return 0;
}
