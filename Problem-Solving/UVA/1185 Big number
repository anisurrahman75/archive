#include<bits/stdc++.h>
using namespace std;
int main()
{
    double n,i,m,f=0,t;
    scanf("%lf",&t);
    while(t--){
    scanf("%lf",&n);
    {
        m=0;
        for(i=1; i<=n; i++)
        {
            m+=log10(i);
        }
        f=floor(m)+1;
        printf("%.0lf\n",f);
    }}
    return 0;
}
