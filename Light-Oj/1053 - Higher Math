#include<iostream>
#include<stdio.h>
#include<math.h>
using namespace std;
int main ()
{
    int t,a,b,c;
    scanf("%d",&t);
    for ( int i=1;i<=t;i++)
    {
        scanf("%d %d %d",&a,&b,&c);
if (a>=b && a>=c)
{
    if((b*b)+(c*c)==(a*a))
    {
        printf("Case %d: yes\n",i);
    }
    else
     printf("Case %d: no\n",i);
}
else if (b>=a && b>=c)
{
    if((c*c)+(a*a)==(b*b))
    {
        printf("Case %d: yes\n",i);
    }
    else
     printf("Case %d: no\n",i);
}
else
{
    if((a*a)+(b*b)==(c*c))
    {
        printf("Case %d: yes\n",i);
    }
    else
     printf("Case %d: no\n",i);
}
    }


    return 0;
}
