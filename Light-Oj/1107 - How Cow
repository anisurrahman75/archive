#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    int t,n;
    scanf("%d",&t);
    int i,j;
    for(i =1; i<=t; i++)
    {
        int x1,x2,y1,y2,a,b;
        printf("Case %d:\n",i);
        scanf("%d%d%d%d",&x1,&y1,&x2,&y2);
        scanf("%d",&n);
        for(j=0; j<n; j++)
        {
            scanf("%d%d",&a,&b);

            if((a>=x1&&a<=x2)&&(b>=y1&&b<=y2))
            {
                printf("Yes\n");
            }
            else
                printf("No\n");


        }
    }
    return 0;

}
